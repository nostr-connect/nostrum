{
  "expo": {
    "name": "nostrum",
    "slug": "nostrum",
    "version": "0.0.1",
    "privacy": "public",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "owner": "nostr-connect",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#050A30",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vulpem.nostrum",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.vulpem.nostrum"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b63ca480-90bb-491c-a8e2-23e48e9d98ae"
      }
    }
  }
}
